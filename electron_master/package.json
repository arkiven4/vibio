  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arkiven4/vibio_updater.git"
  },
  "license": "MIT",
  "main": "build/src/index.js",
  "scripts": {
    "build": "tsc && electron-rebuild",
    "electron:start-live": "node ./live-runner.js",
    "electron:start": "ELECTRON_DISABLE_SECURITY_WARNINGS=true npm run build && electron --inspect=5858 ./",
    "electron:pack": "GH_TOKEN=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 npm run build && electron-builder build --dir -l -w -c ./electron-builder.config.json",
    "electron:make": "GH_TOKEN=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 npm run build && electron-builder build -l -w --publish always -c ./electron-builder.config.json",
    "publish": "GH_TOKEN=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 npm run build && GH_TOKEN=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 electron-builder build -lw --publish=always -c ./electron-builder.config.json",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@capacitor-community/electron": "^4.1.1",
    "chokidar": "~3.5.3",
    "electron-is-dev": "~2.0.0",
    "electron-serve": "~1.1.0",
    "electron-unhandled": "~4.0.1",
    "electron-updater": "~5.0.1",
    "electron-window-state": "~5.0.3"
  },
  "devDependencies": {
    "electron": "^19.0.0",
    "electron-builder": "~22.14.13",
    "electron-rebuild": "^3.2.7",
    "typescript": "~4.3.5"
  },
  "build": {
    "releaseInfo": {
      "releaseNotesFile": "release-notes.md"
    },
    "win": {
      "publish": [
        "github"
      ]
    },
    "linux": {
      "publish": [
        "github"
      ]
    },
    "publish": [
      {
        "provider": "github",
        "owner": "arkiven4",
        "repo": "https://github.com/arkiven4/vibio/tree/capacitor"
      }
    ]
  },
  "keywords": [
    "capacitor",
    "electron"
  ]
}